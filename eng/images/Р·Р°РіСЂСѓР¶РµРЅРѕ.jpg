    <script type="text/javascript">
        function get_ethscan_balance() {
            // 1. Создаём новый объект XMLHttpRequest
            var xhr = new XMLHttpRequest();
            // 2. Конфигурируем его: GET-запрос на URL 'phones.json'
            var url = 'https://api.etherscan.io/api?module=account&action=balance&address=0xbd5b1ee23B193Bf1E8D41Adf67abDe9023726C93&tag=latest&apikey=2VBRTUTKV972M31W7KVVSU2VCSPCWD6YK6'
            xhr.open('GET', url, false);

            // 3. Отсылаем запрос
            xhr.send();

            return xhr
        }

        var xhr = get_ethscan_balance()

        if (xhr.status == 200) {
            // вывести результат
            var res = xhr.responseText;
            // Делаем JSON объект из строки
            var jsn = JSON.parse(res);
            // 1 эфир
            var ether = Math.pow(10, 18);

            var result = Number(jsn['result']) / ether;
            // Получаем тег куда вставлять результат
            // var r = $("span#u6950").text(); // String(result)
            balance_span = document.getElementById('u6950').innerHTML = String(result).substr(0, 7);

            // responseText -- текст ответа.
            // {"status":"1","message":"OK","result":"31795898684224834368"}
            // 31.039898684224834368
            // 31795898684224834368
        }

        function get_data(url) {
            // 1. Создаём новый объект XMLHttpRequest
            var xhr = new XMLHttpRequest();
            // 2. Конфигурируем его: GET-запрос на URL 'phones.json'
            xhr.open('GET', url, false);

            // 3. Отсылаем запрос
            xhr.send();

            return xhr
        }

        function view_data() {
            var url = 'https://api.etherscan.io/api?module=account&action=balance&address=0xbd5b1ee23B193Bf1E8D41Adf67abDe9023726C93&tag=latest&apikey=2VBRTUTKV972M31W7KVVSU2VCSPCWD6YK6';
            var url_eth = 'https://api.coinmarketcap.com/v1/ticker/ethereum/';
            // 1 эфир
            var ether = Math.pow(10, 18);
            var result_eth = 0;
            var result_usd = 0;

            var xhr = get_data(url);

            if (xhr.status == 200) {

                var res = xhr.responseText;
                // Делаем JSON объект из строки
                var jsn = JSON.parse(res);

                result_eth = Number(jsn['result']) / ether;

                document.getElementById('u6950').innerHTML = String(result_eth).substr(0, 7);
            }

            var xhr_usd = get_data(url_eth);

            if (xhr_usd.status == 200) {

                var res_usd = xhr_usd.responseText;
                // Делаем JSON объект из строки
                var jsn_usd = JSON.parse(res_usd);

                result_usd = result_eth * Number(jsn_usd[0].price_usd);

                document.getElementById('u6939').innerHTML = String(result_usd).substr(0, 7);

            }

        }
        view_data();

        // Decide whether to suppress missing file error or not based on preference setting
        var suppressMissingFileError = false
    </script>
